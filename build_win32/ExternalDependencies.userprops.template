    <BOOST_PATH>path_to\boost_trunk</BOOST_PATH>
    <ZLIB_PATH>path_to\zlib-1.2.8</ZLIB_PATH>
    <FRAMEWORK_PATH>path_to\Framework</FRAMEWORK_PATH>
  </PropertyGroup>
  <PropertyGroup />
